def archiveObject(filename, object):
    file = open(filename, "wb")
    pickle.dump(object, file)
    file.close()

def unarchiveObject(filename):
    file = open(filename, "rb")
    object = pickle.load(file)
    file.close()
    return object

print unarchiveObject(file)
